<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string></string>
	<key>CFBundleExecutable</key>
	<string></string>
	<key>NSCameraUsageDescription</key>
	<string>SVD_ROS_Comms needs camera access to detect ArUco markers for spatial localization.</string>
	<key>NSWorldSensingUsageDescription</key>
	<string>SVD_ROS_Comms needs world sensing to track your position and display drill sites in AR.</string>
	<key>NSLocalNetworkUsageDescription</key>
	<string>SVD_ROS_Comms needs local network access to connect to the ROS server for receiving drill site data.</string>
